{
  "name": "Cornell Dining Nutrition Tooltip",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Shows nutrition info in a tooltip when hovering over menu items.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "*://scl.cornell.edu/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://scl.cornell.edu/*"],
      "js": ["content.js"]
    }
  ]
}
